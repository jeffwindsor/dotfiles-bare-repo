{
  "title": "JWM Personal Keys",
  "author": "Jeff Windsor",
  "rules": [
    {
      "description": "CapsLock as Escape / Ctrl+Meta+Alt",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "modifiers": [
                "left_control",
                "left_option"
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Browsers Launch",
      "manipulators": [
        {
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Brave'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "b",
            "modifiers": {
