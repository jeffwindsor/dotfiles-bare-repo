    {
      "description": "Terminals Launch",
      "manipulators": [
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "osascript -e 'tell application \"System Events\" \n if (get name of every application process) contains \"iTerm2\" then \n tell application \"iTerm\" \n create window with default profile \n end tell \n else \n tell application \"iTerm\" to activate \n end if \n end tell'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ]
            }
          },
          "to": [
